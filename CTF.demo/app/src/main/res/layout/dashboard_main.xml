<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <android.support.design.widget.AppBarLayout android:layout_height="wrap_content"
        android:layout_width="match_parent" android:theme="@style/AppTheme.AppBarOverlay"
        android:id="@+id/tableLayout2">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="right">

            <TextView
                android:layout_width="336dp"
                android:layout_height="64dp"
                android:text="DashBoard"
                android:id="@+id/textView2"
                android:textSize="50dp"
                android:gravity="right" />

            <android.support.v7.widget.Toolbar android:id="@+id/toolbar"
                android:layout_width="match_parent" android:layout_height="?attr/actionBarSize"
                android:background="?attr/colorPrimary" app:popupTheme="@style/AppTheme.PopupOverlay" />

        </TableRow>

    </android.support.design.widget.AppBarLayout>

    <android.support.v4.widget.DrawerLayout
        android:id="@+id/drawerLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/tableLayout2">

        <TableLayout
            android:layout_width="fill_parent"
            android:layout_height="427dp"
            android:stretchColumns="0,1"
            android:elevation="4dp"
            android:layout_weight="0">

            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_gravity="center"
                android:layout_weight="0"
                android:layout_marginTop="20dp">

                <TextView
                    android:layout_width="50dp"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="1B16"
                    android:id="@+id/textView3"
                    android:textSize="40dp"
                    android:layout_marginLeft="20dp"
                    android:layout_marginBottom="0dp"
                    android:layout_span="0"
                    android:layout_column="0"
                    android:height="90dp"
                    android:layout_gravity="center_vertical" />

                <ImageView
                    android:id="@+id/computer_icon"
                    android:contentDescription="@string/computers_available"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:src="@drawable/computers_available_true"
                    android:layout_weight="1"/>

                <ImageView
                    android:id="@+id/printer_working"
                    android:contentDescription="@string/printer_working"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:src="@drawable/ic_printer_alert"
                    android:layout_weight="1"
                    android:layout_marginRight="20dp"
                    android:layout_marginBottom="5dp" />
            </TableRow>

            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_gravity="center_horizontal"
                android:layout_weight="0">

                <TextView
                    android:layout_width="50dp"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="1B17"
                    android:id="@+id/textView7"
                    android:textSize="40dp"
                    android:layout_marginLeft="20dp"
                    android:height="90dp"
                    android:layout_gravity="center_vertical" />

                <ImageView
                    android:id="@+id/computer_icon21"
                    android:contentDescription="@string/computers_available"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:src="@drawable/computers_available_true"
                    android:layout_weight="1"/>

                <ImageView
                    android:id="@+id/printer_workin2g1"
                    android:contentDescription="@string/printer_working"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:src="@drawable/ic_printer_alert"
                    android:layout_weight="1"
                    android:layout_marginRight="20dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="5dp" />

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="40dp"
                android:layout_weight="0">

                <TextView
                    android:layout_width="50dp"
                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="1B18"
                    android:id="@+id/textView8"
                    android:textSize="40dp"
                    android:layout_marginLeft="20dp"
                    android:height="90dp"
                    android:layout_gravity="center_vertical" />

                <ImageView
                    android:id="@+id/computer_icon1"
                    android:contentDescription="@string/computers_available"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:src="@drawable/computers_available_true"
                    android:layout_weight="1"/>

                <ImageView
                    android:id="@+id/printer_working1"
                    android:contentDescription="@string/printer_working"
                    android:layout_width="100dp"
                    android:layout_height="100dp"
                    android:src="@drawable/ic_printer_alert"
                    android:layout_weight="1"
                    android:layout_marginRight="20dp"
                    android:layout_marginTop="5dp" />
            </TableRow>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="User: John Doe"
                        android:id="@+id/textView4" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="Quota Remaining"
                        android:id="@+id/textView5"
                        android:singleLine="true" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:text="Last Print Job: 2016/01/15 16:20"
                        android:id="@+id/textView6" />
                </LinearLayout>
            </RelativeLayout>

        </TableLayout>
    </android.support.v4.widget.DrawerLayout>

</RelativeLayout>